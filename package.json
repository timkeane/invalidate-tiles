  },
  "author": "Tim Keane <t.d.keane@gmail.com> (https://timkeane.github.io/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
